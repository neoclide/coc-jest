{
  "name": "coc-jest",
  "version": "1.0.4",
  "description": "jest extension for coc.nvim",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "webpack",
    "prepare": "npx npm-run-all clean build"
  },
  "engines": {
    "coc": ">=0.0.23"
  },
  "activationEvents": [
    "*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neoclide/coc-jest.git"
  },
  "keywords": [
    "coc.nvim",
    "jest"
  ],
  "author": "chemzqm@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neoclide/coc-jest/issues"
  },
  "homepage": "https://github.com/neoclide/coc-jest#readme",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Jest",
      "properties": {
        "jest.terminalPosition": {
          "type": "string",
          "description": "Position of terminal window.",
          "default": "right",
          "enum": [
            "bottom",
            "right"
          ]
        },
        "jest.watch": {
          "type": "boolean",
          "default": true
        },
        "jest.detectLeaks": {
          "type": "boolean",
          "default": false
        },
        "jest.watchman": {
          "type": "boolean",
          "default": false
        },
        "jest.detectOpenHandles": {
          "type": "boolean",
          "default": false
        },
        "jest.forceExit": {
          "type": "boolean",
          "default": false
        },
        "jest.noStackTrace": {
          "type": "boolean",
          "default": false
        },
        "jest.configFileName": {
          "type": "string",
          "default": "jest.config.js"
        },
        "jest.customFlags": {
          "type": "array",
          "description": "Pass additional custom flags to Jest command.",
          "default": [],
          "items": {
            "type": "string",
            "description": "A flag, without the '--'. For example: 'runInBand' or 'useStderr'.",
            "default": ""
          }
        }
      }
    },
    "commands": [
      {
        "title": "Run jest for current project.",
        "category": "Jest",
        "command": "jest.projectTest"
      },
      {
        "title": "Run jest for current file",
        "category": "Jest",
        "command": "jest.fileTest"
      },
      {
        "title": "Run jest init in current vim's CWD",
        "category": "Jest",
        "command": "jest.init"
      }
    ]
  },
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.18",
    "@types/node": "^13.11.1",
    "@types/which": "^1.3.2",
    "coc.nvim": "^0.0.77",
    "rimraf": "^3.0.2",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "which": "^2.0.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {}
}
